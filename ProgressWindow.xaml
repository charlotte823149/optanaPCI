<Window x:Class="optanaPCI.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:optanaPCI"
        mc:Ignorable="d"
        Title="匯出進度" Height="200" Width="600" WindowStartupLocation="CenterScreen">
    <Grid>
        <ProgressBar x:Name="bar" HorizontalAlignment="Left" Height="30" Margin="42,87,0,0" VerticalAlignment="Top" Width="500" Value="-1" Foreground="#FF1AC138"/>
        <Label x:Name="ratio" Content="{Binding Value, ElementName=bar}" HorizontalAlignment="Left" Margin="502,57,0,0" VerticalAlignment="Top" Width="55" FontSize="16"/>
        <Label Content="%" HorizontalAlignment="Left" Margin="533,57,0,0" VerticalAlignment="Top" FontSize="16"/>

    </Grid>
</Window>
